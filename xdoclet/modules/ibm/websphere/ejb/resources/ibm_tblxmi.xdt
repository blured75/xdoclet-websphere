        xmlns:RDBSchema="RDBSchema.xmi" 
		xmi:id="RDBTable_<XDtClass:classTagValue tagName='ejb:persistence' paramName='table-name'/>"
		name="<XDtClass:classTagValue tagName='ejb:persistence' paramName='table-name'/>" 
